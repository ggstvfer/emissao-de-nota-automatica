{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/retry.js": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/retry",
      "schedule": "0 12 * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}