{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "functions": {
    "api/index.mjs": {
      "maxDuration": 30
    },
    "api/retry.mjs": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/retry.mjs",
      "schedule": "0 12 * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.mjs"
    }
  ]
}