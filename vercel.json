{
  "version": 2,
  "buildCommand": "npm run build-all",
  "installCommand": "npm install",
  "functions": {
    "api/nfse-working.mjs": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/nfse-working.mjs" },
    { "source": "/(.*)", "destination": "/ui/dist/index.html" }
  ]
}